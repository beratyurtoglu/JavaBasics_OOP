//Even or Odd
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
    	Scanner scanner = new Scanner(System.in);
        int sayi;
        
        System.out.println("Sayi gir: ");
        sayi = scanner.nextInt();
        
        if(sayi%2 == 0) {
        	System.out.println("sayi cift");
        }
        
        else {
        	System.out.println("sayi tek");
        }               
  }
}
